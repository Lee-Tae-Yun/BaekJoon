#include <iostream>
#include <sstream>
#include <algorithm> 
using namespace std;



int main(){
    int n, num, max = 0;

    int a[9];
    for(int i = 0; i < 9; i++){
        cin >> a[i];
        if(a[i]>max){
            max = a[i];
            num = i+1;
        }
    }
    cout << max << "\n" << num << "\n";

    return 0;
}
