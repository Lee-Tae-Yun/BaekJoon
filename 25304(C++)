#include <iostream>
using namespace std;


int main(){

    int value,sum,count,x,y;
    cin >> value;
    cin >> count;
    sum = 0;
    for(int i = 0; i < count; i++){
        cin >> x >> y;
        sum = sum + x * y;
    }
    if(value == sum){
        cout << "Yes" << endl;
    }
    else{
        cout << "No" << endl;
    }
    return 0;
}
