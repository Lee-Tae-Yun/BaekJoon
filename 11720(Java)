import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        int a,x = 0;
        String s;
        Scanner sc = new Scanner(System.in);
        a = sc.nextInt();
        sc.nextLine();
        s = sc.nextLine();
        for(int i = 0; i < a; i++){
            x += s.charAt(i) - '0';
        }
        System.out.println(x);

    }
}
