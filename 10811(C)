#include <stdio.h>

int main(int argc, const char * argv[])
{
    int n = 0;
    int m= 0;
    int start = 0;
    int end = 0;
    int arr[1000] = { 0 };


    scanf("%d %d", &n, &m);
    
    for (int i = 0; i < n; i++)
    {
        arr[i] = i+1;
    }
    
    for (int i = 0; i < m; i++)
    {
        scanf("%d %d", &start, &end);
        start--;
        end--;
        for (int i = start; i <= end; i++){
            int temp = arr[i];
            arr[i] = arr[end];
            arr[end] = temp;
            end--;
        }
    }
    for (int i = 0; i < n; i++)
    {
        printf("%d ", arr[i]);
    }
    return 0;
}
