import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        int a;
        String s;
        Scanner sc = new Scanner(System.in);
        a = sc.nextInt();
        s = sc.nextLine();

        for(int i = 0; i < a; i++) {
            s = sc.nextLine();
            System.out.println(s.charAt(0)+""+s.charAt(s.length()-1));
        }

    }
}
